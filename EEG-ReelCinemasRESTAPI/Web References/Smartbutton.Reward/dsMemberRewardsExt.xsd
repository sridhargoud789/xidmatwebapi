<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://tempuri.org/dsMemberRewardsExt.xsd" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:mstns="http://tempuri.org/dsMemberRewardsExt.xsd" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://tempuri.org/dsMemberRewardsExt.xsd" id="dsMemberRewardsExt" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element msdata:IsDataSet="true" msdata:UseCurrentLocale="true" name="dsMemberRewardsExt">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="tblMemberRewardsExt">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="RewardId" type="xs:int" />
              <xs:element name="RewardName">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="120" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element minOccurs="0" name="RewardTypeExternalRef">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="40" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="LocationName">
