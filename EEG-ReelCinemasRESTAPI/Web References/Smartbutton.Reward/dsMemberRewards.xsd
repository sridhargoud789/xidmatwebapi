<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="https://app.smartbutton.com/Services/dsMemberRewards.xsd" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:mstns="https://app.smartbutton.com/Services/dsMemberRewards.xsd" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="https://app.smartbutton.com/Services/dsMemberRewards.xsd" id="dsMemberRewards" xmlns:xs="http://www.w3.org/2001/XMLSchema">
